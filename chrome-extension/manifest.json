{
  "manifest_version": 2,
  "name": "Emoji Autocomplete",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "material.min.css"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "textarea-caret-position.js",
        "utils.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "commands": {
    "emoji-auto-complete": {
      "suggested_key": {
        "default": "Ctrl+Space"
      },
      "description": "Find Suggested Emojis"
    }
  },
  "web_accessible_resources": [
    "material.min.js",
    "popup.html"
  ]
}
